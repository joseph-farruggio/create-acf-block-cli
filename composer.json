{
    "name": "joeyfarruggio/create-acf-block",
    "description": "A CLI for scaffolding ACF blocks.",
    "keywords": [
        "ACF",
        "Advanced Custom Fields",
        "WordPress",
        "ACF Blocks",
        "cli"
    ],
    "homepage": "https://joeyfarruggio.com",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/joseph-farruggio/create-acf-block-cli/issues",
        "source": "https://github.com/joseph-farruggio/create-acf-block-cli/"
    },
    "authors": [
        {
            "name": "Joey Farruggio",
            "email": "joey@joeyfarruggio.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "intonate/tinker-zero": "^1.2",
        "laravel/prompts": "^0.1.4",
        "nunomaduro/termwind": "^1.15.1",
        "laravel-zero/framework": "^10.0.2",
        "laravel/pint": "^1.8",
        "mockery/mockery": "^1.5.1",
        "pestphp/pest": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": [
        "builds/create-acf-block"
    ]
}